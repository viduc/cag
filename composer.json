{
  "name": "viduc/cag",
  "description": "Clean Architecture Generator - outil de génération de structure pour la mise en place d'une architecture proproe",
  "type": "library",
  "homepage": "https://viduc.github.com",
  "time": "2022-11-01",
  "authors": [
    {
      "name": "Tristan Fleury",
      "email": "viduc@mail.fr",
      "homepage": "https://viduc.github.com",
      "role": "Developper - Créateur"
    }
  ],
  "license": "GPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "Cag\\": ["src/"],
      "App\\": ["app/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spec\\": ["spec"]
    }
  },
  "scripts": {
    "test": "kahlan --reporter=verbose",
    "coverage": "kahlan --reporter=verbose --coverage=4 --clover=clover.xml"
  },
  "bin": ["bin/cag"],
  "require": {
    "minicli/minicli": "^3.2",
    "minicli/command-help": "^0.1.0",
    "league/container": "*"
  },
  "require-dev": {
    "kahlan/kahlan": "5.*",
    "kahlan/extra-matcher": "1.*",
    "phpunit/phpunit": "9.*",
    "friendsofphp/php-cs-fixer": "^3.13",
    "phpmd/phpmd": "^2.13",
    "squizlabs/php_codesniffer": "^3.7"
  }
}
